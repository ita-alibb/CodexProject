   <children>
      <TextField fx:id="usernameTextField" layoutX="220.0" layoutY="174.0" promptText="username" />
      <Label layoutX="220.0" layoutY="157.0" text="Enter your username: " />
      <Button fx:id="loginButton" layoutX="220.0" layoutY="207.0" mnemonicParsing="false" onAction="#login" text="enter" />
   </children>
</AnchorPane>
